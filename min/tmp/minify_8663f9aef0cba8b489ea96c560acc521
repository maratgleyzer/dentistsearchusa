$(function(){$('a.open-popup').simpleLightbox({faderOpacity:0.7,faderBackground:'#fff',closeLink:'a.close'});});;function hideFormText(){var _inputs=document.getElementsByTagName('input');var _txt=document.getElementsByTagName('textarea');var _value=[];if(_inputs){for(var i=0;i<_inputs.length;i++){if(_inputs[i].className.match(/hide-text-inputs/)){if(_inputs[i].type=='text'||_inputs[i].type=='password'){_inputs[i].index=i;_value[i]=_inputs[i].value;_inputs[i].onfocus=function(){if(this.value==_value[this.index])
